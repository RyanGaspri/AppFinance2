<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cor_fundo"
    android:orientation="vertical"
    tools:context="com.example.calcprojetoint.sobrenos">

    <ImageButton
        android:id="@+id/linkeinbotao"
        android:layout_marginTop="20dp"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        android:src="@drawable/seilaremovebgpreview"
        android:scaleType="centerCrop"
        android:background="@android:color/transparent"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textSize="35dp"
        android:text="Ryan Gaspri"
        android:gravity="center"
        android:textColor="@color/black"/>

    <ImageButton
        android:id="@+id/botaoGabriel"
        android:layout_marginTop="20dp"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        android:src="@drawable/seilaremovebgpreview"
        android:scaleType="centerCrop"
        android:background="@android:color/transparent"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textSize="35dp"
        android:text="Gabriel Gato"
        android:gravity="center"
        android:textColor="@color/black"/>

    <ImageButton
        android:id="@+id/BotaoVictor"
        android:layout_marginTop="20dp"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        android:src="@drawable/seilaremovebgpreview"
        android:scaleType="centerCrop"
        android:background="@android:color/transparent"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textSize="35dp"
        android:text="Victor Santos"
        android:gravity="center"
        android:textColor="@color/black"/>

    <ImageButton
        android:layout_marginTop="20dp"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        android:src="@drawable/seilaremovebgpreview"
        android:scaleType="centerCrop"
        android:background="@android:color/transparent"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textSize="35dp"
        android:text="Bruno Reis"
        android:gravity="center"
        android:textColor="@color/black"/>




</LinearLayout>